rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /lrs/{lrId} {
      allow read, write: if request.auth != null &&
        (request.resource.data.ownerUid == request.auth.uid || resource.data.ownerUid == request.auth.uid);
    }
    match /presets/{collection}/{doc} { allow read, write: if request.auth != null; }
  }
}
